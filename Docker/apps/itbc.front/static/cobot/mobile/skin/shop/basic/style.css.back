.m_service_info .list_wrap ul li .title.hm { width:calc(100% - 40px); }
/*수익실현종목현*/
.m_total_profit { margin:30px 12px 0; }
.m_total_profit ul { margin-top:3px; }
.m_total_profit ul:after { content:''; clear:both; display:block; }
.m_total_profit > ul > li { padding:4px; float:left; width:50%; }
.m_total_profit > ul > li > div { border:1px #ebebec solid; padding:10px 13px; letter-spacing:-0.66px; }
.m_total_profit > ul > li > div a { display:block; }
.m_total_profit > ul > li > div a:after { content:''; clear:both; display:block; }
.m_total_profit .title { color:#0d162a; font-weight:700; font-size:11px; white-space: nowrap; overflow: hidden; text-overflow: ellipsis; }
.m_total_profit .p_point { color:#f63440; font-size:20px; margin:7px 0 12px; }
.m_total_profit > ul ul li { float:left; width:50%; }
.m_total_profit > ul ul li span { width:100%; display:block;  }
.m_total_profit > ul ul li span:first-child { letter-spacing:-0.3px; color:#a49293; font-size:7px; }
.m_total_profit > ul ul li span:last-child { color:#0d162a; font-size:10px; letter-spacing:-0.6px; margin-top:3px;  }
/*구성*/
.m_member_info { }
.m_member_info img { max-width:100%; margin-top:12px; }
/*고맞춤형서비스*/

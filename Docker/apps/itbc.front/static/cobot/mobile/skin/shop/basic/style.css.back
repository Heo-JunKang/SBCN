/*연구*/
.m_laboratory { }
.m_laboratory img { max-width:100%; display:block; margin-top:12.33px; margin-bottom:29.66px; }
/*footer*/
.m_footer { background:#f4f2f2;color:#0d162a; padding:15.66px 16.33px; font-size:9px; letter-spacing:-0.53px; line-height:16.33px; }
.m_footer .f_links { margin:26px 0; }
.m_footer .f_links a { font-weight:700;   }

@charset "utf-8";
/* SIR 지운아빠 */

/* 201800412-mobile main------------------------------------------------------------------------ */
body { background:#fff; }
body > div > img.draggable { max-width:100%; }
.clear { clear:both; }
/* top 버튼*/
#nav_up { position:fixed; bottom:10px; right:10px; z-index:10; display:none;border: 1px #a2a2a2 solid;background: #a2a2a2; width:35px; height:35px;  overflow:hidden; opacity: 0.7;}
#nav_up:before {content: '';position: absolute;width: 10px;height: 10px;border: 1px #fff solid;border-width: 2px 2px 0 0;display: block;transform: rotate(-45deg);top: 12px;left: 12px;}
#nav_up:after { content: '';position: absolute;width: 2px;height: 14px;display: block;background: #fff;top: 12px;left: 16px;}
#nav_up span { text-indent:-999px; width:35px; height:35px; display:inline-block; overflow:hidden;}
/* sub - 서비스신청완료 페이지*/
h1.sub_title {background:#0d162a; text-align:center; font-size:12px; color:#fff; padding:0 10px; line-height:40px; letter-spacing:-0.6px;}
h1.sub_title .m_back_btn { overflow:hidden;text-indent:-99999px; width: 10px;height: 10px;  border: #fff 1px solid;border-width: 2px 2px 0 0;transform: rotate(225deg);position: absolute;left: 16px; top: 15px; }
.request_complete { margin:32px auto 16px; }
.request_complete h3 { font-size:20px; margin:0 0 16px; }
.request_complete > p { font-size:14px; margin-bottom:32px; }
div.fees_info { font-size:12px; padding:0 16px; letter-spacing:-0.5px; line-height:1.3; }
.fees_info table { width:100%; margin:16px auto 0; }
.fees_info table td { width:auto;height:auto;padding:16px 5px;}
.request_complete .btn_wrap { margin-top:16px; padding-bottom:16px; }
.request_complete .btn_wrap span {width:80px; }
.request_complete .btn_wrap span:last-child {width:140px; margin-left:16px; background: #f63440;border: solid 1px #f63440;}
/* header */
.m_header {text-align:center; position:relative; background:#fff; }
.m_header.fixed { padding-top:89px; }
.m_header .head_line { padding:10px 0 0; height:89px; box-sizing:border-box; }
.m_header.fixed .head_line { position:fixed; top:0; width:100%; background:rgba(255,255,255,0.95); z-index:10;box-sizing:border-box; -webkit-box-shadow: 0 0 0 1px rgba(0,0,0,0.05);box-shadow: 0 0 0 1px rgba(0,0,0,0.05);}
.m_header h1 { display:inline-block; }
.m_header h1 img { margin:0; clear:both; max-height:21px; vertical-align:baseline; }
.m_header .m_nav_ico { position:absolute; top:14px; right:16px; }
.m_header .m_nav_ico img { max-height:12px; vertical-align:baseline; }
.m_header ul.gnb { height:46px; float:none; padding:10px 0 0 ; }
.m_header ul.gnb:before { content:''; display:block; clear:both; }
.m_header ul.gnb li { margin:0; width:33.33%; float:left; }
.m_header ul.gnb li a { font-size:14px; color:#0d162a; letter-spacing:-0.8px; padding:0; display:block; padding:16px 0; font-family: 'Noto Sans KR',sans-serif; font-weight:600; }
.m_header ul.gnb li a.on { color:#f63440; position:relative; }
.m_header ul.gnb li a.on:after { content:''; display:block; position:absolute; width:100%; height:2.5px; background:#f63440;left:0;right:0;margin:auto;bottom:0px;}
/*m_menu slide*/
.slide_bg { z-index:13; position:fixed; top:0; bottom:0; left:0; right:0; background:rgba(0,0,0,0.5); display:none; }
.rock_s { overflow:hidden; }
#menu_slide_box { color:#0d162a; background:#fff; width:80%; position:fixed; top:0; bottom:0; right:-80%; margin:0; z-index:15; }
#menu_slide_box .info_area { height:45px; padding:14px; }
#menu_slide_box .info_area a.user_btn { color:#0d162a; font-weight:700; font-size:14px; letter-spacing:-0.3px; }
#menu_slide_box .info_area a#slide_close_btn { float:right; position:relative; width:14px; height:14px; display:inline-block; }
#menu_slide_box .info_area a#slide_close_btn span { position:absolute; top:0; left:6px; width:2px; height:18px; background:#0d162a; display:inline-block; transform: rotate(45deg);}
#menu_slide_box .info_area a#slide_close_btn span:last-child { width:2px; height:18px; background:#0d162a; display:inline-block; transform: rotate(-45deg);}
#menu_slide_box .slide_nav { height:45px; border:1px #a2a2a2 solid; border-width:1px 0 1px 0; position:relative; }
#menu_slide_box .slide_nav:before { content:''; width:1px; height:14px; background:#a2a2a2; display:block; position:absolute; left:50%; top:0; bottom:0; margin:auto; }
#menu_slide_box .slide_nav:after { content:''; clear:both; display:block; }
#menu_slide_box .slide_nav ul li { width:50%; float:left;  }
#menu_slide_box .slide_nav ul li a { line-height:45px; font-size:14px; font-weight:700; letter-spacing:-0.833px; color:#0d162a; height:100%; display:block; text-align:center; }
#menu_slide_box .join_box { padding-top:58px; text-align:center; color:#0d162a; font-size:14px; letter-spacing:-0.833px; }
#menu_slide_box .join_box p { margin-bottom:15px; }
#menu_slide_box .join_box a { border-bottom:1px #0d162a solid; font-weight:700; line-height:30px; }
#menu_slide_box .tab_box .tab_link { height:43px; padding:0 16px;}
#menu_slide_box .tab_box span { line-height:43px; font-size:14px; letter-spacing:-0.833px;  }
#menu_slide_box .tab_box span.on { font-weight:700; }
#menu_slide_box .tab_box span:before { content:'|'; display:inline-block; margin:0 5px;  }
#menu_slide_box .tab_box span:first-child:before { display:none; }
#menu_slide_box .tab_box .content { background:#fcf9f9; padding:6px 15px 15px; font-size:14px; letter-spacing:-0.633px;  }
#menu_slide_box .tab_box .content li { display:none; }
#menu_slide_box .tab_box .content li.on { display:block; }
#menu_slide_box .tab_box .content dt { width:calc(100% - 40px); float:left; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
#menu_slide_box .tab_box .content dl:after { content:''; display:block; clear:both;}
#menu_slide_box .tab_box .content dt, #menu_slide_box .tab_box .content dd { margin-top:9px; }
#menu_slide_box .tab_box .content dd { width:40px; text-align:right; float:left;  }
#menu_slide_box .tab_box .content dd.red { color:#f63440; font-weight:700; }
#menu_slide_box .logout_btn_wrap {position: absolute;bottom: 0;  width: 100%;border-top: #f4f2f2 1px solid; padding: 0 13px;line-height: 43px;font-size: 14px;letter-spacing: -0.633px;display: block; }
#menu_slide_box .acd_tab_box {  }
#menu_slide_box .acd_tab_box > ul > li { border-bottom:#f4f2f2 1px solid;  }
#menu_slide_box .acd_tab_box > ul > li > a { position:relative; padding:0 13px; line-height:43px; font-size:14px;letter-spacing:-0.633px; display:block; position:relative; }
#menu_slide_box .acd_tab_box .acd_true span { width:12px; height:12px; border:#000 1px solid; border-width:2px 2px 0 0; transform: rotate(135deg); transition:all ease 0.4s;position:absolute; right:16px;; top:13px; }
#menu_slide_box .acd_tab_box .acd_true.on span { top:18px;transform: rotate(-45deg); transition:all ease 0.4s; }
#menu_slide_box .acd_tab_box .acd_cont { background:#fcf9f9; padding:12px 16.66px; display:none; }
#menu_slide_box .acd_tab_box .acd_cont table.pf_table { width:100%; color:#a49293; font-size:12px; letter-spacing:-0.733px;  }
#menu_slide_box .acd_tab_box .acd_cont table.pf_table th, #menu_slide_box .acd_tab_box .acd_cont table.pf_table td {padding:4px 0; border-top:1px #f4f2f2 solid; }
#menu_slide_box .acd_tab_box .acd_cont table.pf_table thead th { border:none; }
#menu_slide_box .acd_tab_box .acd_cont table.pf_table th { text-align:left; font-weight:500; }
#menu_slide_box .acd_tab_box .acd_cont table.pf_table thead th { text-align:right; }
#menu_slide_box .acd_tab_box .acd_cont table.pf_table td { text-align:right; }
#menu_slide_box .acd_tab_box .acd_cont table.pf_table td.text-center { text-align:center;}
#menu_slide_box .acd_tab_box .acd_cont table.pf_table thead th:first-child,
#menu_slide_box .acd_tab_box .acd_cont table.pf_table tbody th { border-right:1px #f4f2f2 solid;  }
#menu_slide_box .acd_tab_box .acd_cont.add_link { padding:0; }
#menu_slide_box .acd_tab_box .acd_cont > a.link { font-size:13px; display:block; padding: 12px 16.66px; }
/* main visual */
#hd { border:none; clear:both; min-width:0; }
.main_visual { background-color:#050e25; height:auto; position:relative; overflow:hidden; }
.main_visual .area_set { visibility: hidden;}
.main_visual img { max-width:360px; width:100%; display:block;}
.main_visual .bg_add { background-color:#f63440; width:50%; height:100%; top:0; right:0; position:absolute; }
.main_visual img.visual { position:absolute; left:0; top:0; right:0; bottom:0; margin:0 auto; display:block;}

/* container set */
#container { width:100%; min-height:auto; margin:0; }
.box { margin:30px 16px 0; }
.box h3 { text-align:center; color:#0d162a; letter-spacing:-0.9px; font-size:16px; font-family:'Noto Sans KR'; font-weight:700; }
.box h3 span { color:#f63440;  }
.box .h3_desc { text-align:center; color:#a2a2a2; margin-bottom:5px; letter-spacing:-0.66px; font-size:11px; }
/* 코봇 인공지능 vs 인간 */
.m_service_info .desc_gray { text-align:center; padding-top:9.6px;}
.m_service_info .desc_gray span { font-size:9px; letter-spacing:-0.53px; color:#a2a2a2; font-family:'Noto Sans KR'; font-weight:700; }
.m_service_info .desc_gray span:first-child { margin-right:59px; font-weight:500; }
.m_service_info .desc_gray span:last-child { margin-left:68.33px; font-weight:500; }
.m_service_info .list_wrap { padding:10.5px 0 0; position:relative; }
.m_service_info .list_wrap:after { content:''; clear:both;display:block;  }
.m_service_info .list_wrap ul { width:50%; float:left; font-size:11px; }
.m_service_info .list_wrap ul:first-child:after { content:''; width:1px; position:absolute; top:10.5px; bottom:0; right:50%; background-color:#ebebec; }

/*cs info */
.m_information { font-family:'Noto Sans KR'; }
.m_information .in_box { background:#fcf9f9; border:1px #ebebec solid; padding:10px; color:#0d162a; position:relative;}
.m_information .in_box:first-child { margin-bottom:8px; }

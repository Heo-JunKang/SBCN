.m_footer .copy { }

/*레이어*/
.layer_full_pop { display:none; position:fixed;top:0;left:0;right:0;bottom:0;z-index:20;background:#fff; }
.layer_full_pop .title { background:#0d162a; padding:16px; position:relative; }
.layer_full_pop h2 { color:#fff; font-size:16px; line-height:20px;  }
.layer_full_pop .layer_full_pop_close { float:right;right:16px;top:16px; position:absolute; width:14px; height:14px; display:inline-block; }
